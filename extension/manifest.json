{
  "name": "Tableau Bridge on Linux",
  "version": "0.1.3",
  "description": "Collect information to spin up Bridge-On-Linux agents.",
  "permissions": ["tabs", "cookies"],
  "host_permissions": ["https://*.tableau.com/*", "https://*.tabint.net/*"],
  "background": {},
  "content_scripts": [],
  "web_accessible_resources": [
    {
      "resources": [],
      "matches": ["https://*.tableau.com/*", "https://*.tabint.net/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/ext-icon.png",
      "24": "assets/ext-icon.png",
      "32": "assets/ext-icon.png"
    },
    "default_title": "Tableau Bridge on Linux",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}
